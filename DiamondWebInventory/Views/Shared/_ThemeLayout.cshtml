<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    @{ string version = "1.0.2";}
    @*@Styles.Render("~/Content/css")*@
    @Scripts.Render("~/bundles/modernizr")

    <!--Favicons-->
    <link rel="icon" type="image/png" sizes="32x32" href="favicon/favicon-32x32.png">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="theme-color" content="#ffffff">

    <!--Bootstrap and Other Vendors-->
    <link href="/ThemeContent/css/fontawesome-all.min.css" rel="stylesheet" />
    <link href="/ThemeContent/css/bootstrap.min.css" rel="stylesheet">
    <link href="/ThemeContent/css/bootstrap-theme.min.css" rel="stylesheet">
    <script src="https://code.iconify.design/1/1.0.6/iconify.min.js"></script>
    
    <link href="/ThemeContent/vendors/owl.carousel/css/owl.carousel.min.css" rel="stylesheet" />

    <link href="/ThemeContent/vendors/flexslider/flexslider.css" rel="stylesheet" media="screen" />

    <link href="https://fonts.googleapis.com/css?family=PT+Sans:400,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Questrial" rel="stylesheet">

    <!--Mechanic Styles-->
    <link href="~/AdminContent/assets/css/app-style.css" rel="stylesheet" />

    <link href="/ThemeContent/css/style.css" rel="stylesheet" />
    <link href="/ThemeContent/css/responsive.css" rel="stylesheet" />
    <link href="/AdminContent/assets/css/custom.css" rel="stylesheet" />
    <link href="/ThemeContent/css/Custom.css" rel="stylesheet" />
    
    <script src="~/ThemeContent/js/Angular js/angular.js"></script>
    <script src="~/ThemeContent/js/Angular js/angular-resource.min.js"></script>
    <script src="//angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.11.0.js"></script>

    <style>
    </style>

</head>
<body ng-app="LiveInvApp">
    <div ng-controller="LiveInvCtrl" ng-cloak>

        <div id="loaderWrapper">
            <div id="loader">
                @*<h1>Loading...</h1>*@
                <img src="/AdminContent/assets/images/Spin.gif" />
            </div>
        </div>

        @Html.Partial("_ThemeHeader")
        <div id="wrapper">
            @RenderBody()

            @Html.Partial("_ThemeFooter")
        </div>
    </div>
    <!--jQuery-->
    <script src="/ThemeContent/js/jquery-2.1.3.min.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script src="~/ThemeContent/js/Angular js/angular.min.js"></script>
    <script src="/Content/js/Custom.js?var=@version"></script>

    <script src="/AdminContent/assets/js/popper.min.js"></script>
    <!--Google Maps-->
    <script src="https://maps.googleapis.com/maps/api/js"></script>
    <!--Bootstrap JS-->
    <script src="/ThemeContent/js/bootstrap.min.js"></script>
    <!--Owl Carousel-->
    <script src="/ThemeContent/vendors/owl.carousel/js/owl.carousel.min.js"></script>
    <!--Isotope-->
    <script src="/ThemeContent/vendors/flexslider/isotope-custom.js"></script>
    <!--FlexSlider-->
    <script src="/ThemeContent/vendors/flexslider/jquery.flexslider-min.js"></script>
    <!--D-shine JS-->
    <script src="/ThemeContent/js/ScrollMagic.min.js"></script>
  
    <script src="/ThemeContent/js/jquery.counterup.min.js"></script>
    
    <script src="~/ThemeContent/js/d-shine.js"></script>

    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script type="text/javascript" src="/AdminContent/assets/plugins/jquery-validation/js/jquery.validate.min.js"></script>
</body>
</html>
