<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="/AdminContent/assets/plugins/ion-rangeslider/css/ion.rangeSlider.css" rel="stylesheet" type="text/css" />
    <link href="/AdminContent/assets/plugins/ion-rangeslider/css/ion.rangeSlider.skinFlat.css" rel="stylesheet" type="text/css" />
    <!-- simplebar CSS-->
    <link href="/AdminContent/assets/plugins/simplebar/css/simplebar.css" rel="stylesheet" />
    <link href="/AdminContent/assets/css/app-style.css" rel="stylesheet" />
    <link href="/AdminContent/assets/css/custom.css" rel="stylesheet" />

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Application name", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Inventory", "Inventory", "Home", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Live Inventory", "LiveInventory", "Home", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Home", "Index", "Home", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("API", "Index", "Help", new { area = "" }, null)</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <!--Ion range Slider-->

    <script src="/AdminContent/assets/angular/angular.min.js"></script>
    <script src="/AdminContent/assets/angular/angular-sanitize.js"></script>

    <script src="/Content/js/Custom.js"></script>
    @*<script src="~/Content/js/Client.js"></script>*@

    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>*@
    <script src="/AdminContent/assets/js/popper.min.js"></script>
    <script src="/AdminContent/assets/plugins/ion-rangeslider/js/range-slider-script.js"></script>
    <script src="/AdminContent/assets/plugins/ion-rangeslider/js/ion.rangeSlider.min.js"></script>

    @*<script src="~/AdminContent/assets/plugins/ion-rangeslider/js/range-slider-script.js"></script>*@

</body>
</html>
