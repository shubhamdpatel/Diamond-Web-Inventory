@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>Admin Login</title>
    <!--favicon-->
    <link rel="icon" href="/AdminContent/assets/images/favicon.ico" type="image/x-icon">
    <!-- Bootstrap core CSS-->
    <link href="/AdminContent/assets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- animate CSS-->
    <link href="/AdminContent/assets/css/animate.css" rel="stylesheet" type="text/css" />
    <!-- Icons CSS-->
    <link href="/AdminContent/assets/css/icons.css" rel="stylesheet" type="text/css" />
    <!-- Custom Style-->
    <link href="/AdminContent/assets/css/app-style.css" rel="stylesheet" />
</head>
<body>
    <!-- Start wrapper-->
    <div id="wrapper">
        <div class="card card-primary card-authentication1 mx-auto my-5 animated bounceInDown">
            <div class="card-body">
                <div class="card-content p-2">
                    <div class="card-title text-uppercase text-center pb-2">Sign In</div>
                    <form>
                        <div class="form-group">
                            <div class="position-relative has-icon-right">
                                <label for="txtEmail_Id" class="sr-only">Email Id</label>
                                <input type="text" id="txtEmail_Id" class="form-control form-control-rounded" placeholder="Email Id">
                                <div class="form-control-position">
                                    <i class="icon-user"></i>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="position-relative has-icon-right">
                                <label for="txtPassword" class="sr-only">Password</label>
                                <input type="password" id="txtPassword" class="form-control form-control-rounded" placeholder="Password">
                                <div class="form-control-position">
                                    <i class="icon-lock"></i>
                                </div>
                            </div>
                        </div>
                        <button type="button" class="btn btn-primary btn-round btn-block waves-effect waves-light" onclick="AdminLogin()">Sign In</button>
                        <div class="form-group col-12 text-center mt-3">
                            <span class="text-danger font-weight-bold" id="errormsg"></span>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <!--Start Back To Top Button-->
        <a href="javaScript:void();" class="back-to-top"><i class="fa fa-angle-double-up"></i> </a>
        <!--End Back To Top Button-->
    </div><!--wrapper-->
    <!-- Bootstrap core JavaScript-->
    <script src="/AdminContent/assets/js/jquery.min.js"></script>
    <script src="/AdminContent/assets/js/popper.min.js"></script>
    <script src="/AdminContent/assets/js/bootstrap.min.js"></script>
    <script src="/Content/js/Custom.js"></script>
</body>
</html>
