                            <div class="col-sm-12">
                                <input type="text" class="form-control" id="txtDesignation" name="txtDesignation">
                            </div>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3">
                            <label for="txtDiscount" class="col-sm-12 col-form-label">Discount</label>
                            <div class="col-sm-12">
                                <input type="text" class="form-control" id="txtDiscount" name="txtDiscount">
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <label for="txtAddress" class="col-sm-12 col-form-label">Address</label>
                            <div class="col-sm-12">
