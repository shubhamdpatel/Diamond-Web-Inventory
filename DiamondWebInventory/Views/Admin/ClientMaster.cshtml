@*Banner Form Model*@
<!-- Modal -->
<div class="modal fade" id="ClientMasterModel">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"><i class="fa fa-star"></i>Client Maste</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form id="ClientMasterForm">
                <div class="modal-body">
                    <input type="hidden" id="hdnClientMasteId" value="" />
                    <div class="form-group row">
                        <div class="col-sm-3">
                            <label for="txtLoginName" class="col-sm-12">LoginName</label>
                            <div class="col-sm-12">
                                <input type="text" class="form-control" id="txtLoginName" name="txtLoginName">
                                @*<label for="txtLoginName" id="hdnLoginName "class="col-sm-12">Shubham</label>*@
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <label for="txtPassword" class="col-sm-12">Password</label>
                            <div class="col-sm-12">
                                <input type="text" class="form-control" id="txtPassword" name="txtPassword" multiple>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <label for="txtTitle" class="col-sm-12 col-form-label">Title</label>
                            <div class="col-sm-12">
                                <select class="form-control" id="txtTitle" name="txtTitle">
                                    <option value="" selected hidden disabled>Select</option>
                                    <option value="Mr.">Mr.</option>
                                    <option value="Mrs.">Mrs.</option>
                                    <option value="Miss.">Miss.</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <label for="txtFirstname" class="col-sm-12 col-form-label">Firstname</label>
                            <div class="col-sm-12">
                                <input type="text" class="form-control" id="txtFirstname" name="txtFirstname">
                            </div>
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-3">
                            <label for="txtLastname" class="col-sm-12 col-form-label">Lastname</label>
                            <div class="col-sm-12">
